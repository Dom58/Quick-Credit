{
  "name": "quick-credit",
  "version": "1.0.0",
  "description": "Quick credit is an easy web loan application",
  "main": "app.js",
  "scripts": {
    "test": "nyc --reporter=text --reporter=lcov mocha --require @babel/register server/tests/**/*.test.js --exit",
    "compile": "./node_modules/.bin/babel ./server -d ./build",
    "server": "nodemon --exec babel-node server/app",
    "clean": "rm -rf build && mkdir build",
    "start": "node build/app.js",
    "test-watch": "nodemon --exec npm test",
    "coveralls": "npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dom58/Quick-Credit.git"
  },
  "author": "Ndahimana Dominique",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dom58/Quick-Credit/issues"
  },
  "homepage": "https://github.com/Dom58/Quick-Credit#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-polyfill": "^6.26.0",
    "coveralls": "^3.0.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.0"
  }
}
