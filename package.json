{
  "name": "quick-credit",
  "version": "1.0.0",
  "description": "Quick credit is an easy web loan application",
  "main": "app.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=text --reporter=lcov mocha --require @babel/register --require @babel/polyfill server/tests/**/*.test.js --exit",
    "server": "nodemon --exec babel-node server/app",
    "test-watch": "nodemon --exec npm test",
    "coveralls": "npm test && nyc report --reporter=text-lcov | coveralls",
    "eslint.nodePath": "./node_modules/eslint-config-airbnb-standard/node_modules/eslint",
    "droptables": "nodemon --exec babel-node server/models/dropTables.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dom58/Quick-Credit.git"
  },
  "author": "Ndahimana Dominique",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dom58/Quick-Credit/issues"
  },
  "homepage": "https://github.com/Dom58/Quick-Credit#readme",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "pg": "^7.11.0",
    "psql": "0.0.1",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-standard": "^3.0.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.0"
  }
